<?xml version="1.0" encoding="UTF-8"?>
<Keyboards>
	<!--
	id := a UUID (you may want to generate this using our simple Java program at:  /trunk/Tools/Uuidgen/ (uuidgen_Windows.bat or uuidgen_Linux)
	nameResId := the resource Id which holds the name of the keyboard. See strings.xml.
	layoutResId := the resource Id which holds the layout definition of this keyboard. The sample is an English QWERTY keyboard.
	defaultDictionaryLocale := a locate identifier for this keyboard. E.g., English will be en, Hebrew will be iw, etc.
	description := some text you would like to add.
	index := the sort index of this keyboard in the Settings page.
	 -->
	<Keyboard nameResId="@string/cv_cyrillic_keyboard" 
				iconResId="@drawable/be_cyrillic" 
				layoutResId="@xml/cv_cyrillic" 
				id="733ca6bf-64cf-4233-9bad-e125867b2031" 
				description="Created by A Chuvash Guy" 
				index="1" 
				physicalKeyboardMappingResId="@xml/cyrillic_physical"/>
	<Keyboard nameResId="@string/cv_latin_keyboard" 
				iconResId="@drawable/be_latin" 
				layoutResId="@xml/cv_latin" 
				id="99943e01-5aee-4002-9f82-f8d020fb9133" 
				description="Created by A Chuvash Guy" 
				index="2" 
				physicalKeyboardMappingResId="@xml/latin_physical"/>
</Keyboards>